{
	"name": "AWS Navbar Region Color",
	"version": "1.0.0",
	"description": "Change AWS's navbar color depending on the region",
	"manifest_version": 2,
	"browser_action": {
		"default_title": "Options",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"*://*.console.aws.amazon.com/*"
	],
	"content_scripts": [
		{
		  "matches": ["*://*.console.aws.amazon.com/*"],
		  "js": ["main.js"]
		}
	]
}
